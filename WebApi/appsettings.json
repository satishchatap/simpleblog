{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "PersistenceModule": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=SimpleBlog;Trusted_Connection=True;"
  },
  "FeatureManagement": {
    "ErrorFilter": false,
    "Swagger": true,
    "Authentication": true,
    "SQLServer": true,
    "CreateArticle": true,
    "ArticleLikeCount": true,
    "ArticleCommentCount": true,
    "CommentArticle": true,
    "LikeArticle": true,
    "GetArticle": true,
    "DeleteArticle": true,
    "EditArticle": true,
    "GetArticles": true,
    "GetAllArticles": true
  },
  "AuthenticationModule": {
    "AuthorityUrl": "https://localhost:44302/"
  }
}
